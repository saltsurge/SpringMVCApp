<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                http://www.springframework.org/schema/beans/spring-beans.xsd" 
       default-init-method="initMe"
       default-destroy-method="destroyMe" >
    
   
    <bean name="abstractKlass" class="com.madhusudhan.jscore.fundamentals.inherit.AbstractKlass" abstract="true"/>
    
    <bean name="klass" class="com.madhusudhan.jscore.fundamentals.inherit.Klass" parent="abstractKlass"/>
    
    

        
</beans>